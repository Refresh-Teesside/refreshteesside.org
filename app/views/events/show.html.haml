.photo-container--bravo
  .ui-container--alpha{"data-ui-component" => "view-main-container"}
    / top
    .ui-container--bravo{"data-ui-component" => "view-top-container"}
      / header
      .ui-container.spacing-alpha.border-alpha.align-flex--break{"data-ui-component" => "view-header-container"}
        .grid-three-tenths--break.left.align-flex__item--break
          / brand
          %h1.heading--one
            %img.brand-img{alt: "Refresh Teeside", src: "data:image/png;base64,"}/
          / /brand
        .grid-seven-tenths--break.right.text-right.align-flex__item--break
          / main nav
          %ul.ui-list--alpha{"data-ui-component" => "main-nav-list"}
            %li.ui-list--alpha__item
              %a{href: "#speakers"} Speakers
            %li.ui-list--alpha__item
              %a{href: "#schedule"} Schedule
            %li.ui-list--alpha__item
              %a{href: "#about"} About
            %li.ui-list--alpha__item
              %a{href: "#sponsors"} Sponsors
            %li.ui-list--alpha__item
              %a{href: "#contact"} Contact
          / /main nav
      / /header
      / hero
      .ui-container--delta.text-center{"data-ui-component" => "hero-container"}
        %h1.heading--two.text-light Next Event
        %h2.heading--one.text-light= @event.date.strftime("%B %d %Y")
        %h3.heading--three.text-light.spacing-charlie
          #{@event.date.strftime("%H:%M %p")} • #{@event.location.title}
        .spacing-bravo
          = link_to "Register Now Free", "http://www.eventbrite.co.uk/e/#{@event.eventbrite_id}", class: "ui-button--alpha"
        %i.icon--chevron-down.text-light
      / /hero
      / /top
    / section
    .ui-container--echo.text-center{"data-ui-component" => "main-content-container"}
      / stats
      .ui-container--delta.spacing-delta.alt-container.grid-one-whole{"data-ui-component" => "statistics-container"}
        .ui-container--alpha.align-flex--break{"data-ui-component" => "data-container"}
          .grid-one-half--break.left.align-flex--center.align-flex__item--break
            .text-left.left.spacing-right.spacing-echo.border-charlie.align-flex__item
              %h3.heading--five.text-center.spacing-alpha= @event.sign_up_count
              %span.text-mid Registered
            %ul.grid-eight-tenths.ui-list.image-grid.right.align-flex__item{"data-ui-component" => "attendee-list"}
              %li.image-grid__item
                = image_tag("64afb7e54553a9e529fe1b8275efd1d5.jpeg")
              %li.image-grid__item
                = image_tag("A6-nMCM0.jpeg")
              %li.image-grid__item
                = image_tag("s6ubfHRc.jpeg")
              %li.image-grid__item
                = image_tag("k745U84V.jpeg")
              %li.image-grid__item
                = image_tag("2253fe4234258844d49f3ca6b3b9b68f.jpeg")
              %li.image-grid__item
                %a.ui-button--bravo{href: "http://www.eventbrite.co.uk/e/#{@event.eventbrite_id}"}
                  %i.icon--ellipsis
          .grid-one-half--break.ui-container.text-left.align-flex--center.align-flex__item--break
            .left.spacing-right.spacing-echo.border-charlie.align-flex__item
              %h3.heading--five
                %a{href: "http://twitter.com/refreshteesside"}
                  %i.icon--twitter
              %span.text-mid.text-meta Latest
            .ui-container.align-flex__item
              %p.copy--bravo= @last_tweet.text.html_safe
              %span= time_ago_in_words(@last_tweet.created_at)
      / /stats
      / speakers
      #speakers.ui-container--delta{"data-ui-component" => "speakers-container"}
        .ui-container--charlie
          %h1.heading--four.spacing-bravo.heading-underline This Month's Speakers
        .ui-container--alpha
          %ul.ui-list--bravo{"data-ui-component" => "speaker-list"}
            - @event.speakers.each do |s|
              %li.ui-list__item.grid-one-quarter--break.left
                .ui-list__item__image.spacing-bravo
                  = image_tag(s.file.url)
                  %h2.heading--three=link_to s.name, "http://twitter.com/#{s.twitter}"
            - if @event.speakers.count < 4
              - (4 - @event.speakers.count).times do
                %li.ui-list__item.grid-one-quarter--break.left
                  .ui-list__item__image.spacing-bravo
                    = image_tag("tbc.png")
                    %h2.heading--three to be confirmed

      / /speakers
      / schedule
      #schedule.ui-container--delta.alt-container{"data-ui-component" => "schedule-container"}
        .ui-container--charlie
          %h1.heading--four.spacing-bravo.heading-underline Event Schedule
        %ul.ui-list--charlie{"data-ui-component" => "schedule-list"}
          %li.ui-list--charlie__item
            %h3.heading--three.ui-list--charlie__title.right.text-left Registration
            %span.ui-list--charlie__meta.left.text-right{"data-ui-component" => "schedule-time"} 6:00pm
          %li.ui-list--charlie__item
            %h3.heading--three.ui-list--charlie__title.right Networking
            %span.ui-list--charlie__meta.left{"data-ui-component" => "schedule-time"} 6:30pm
          %li.ui-list--charlie__item
            %h3.heading--three.ui-list--charlie__title.right.text-left Talks
            %span.ui-list--charlie__meta.left{"data-ui-component" => "schedule-time"} 7:00pm
          %li.ui-list--charlie__item
            %h3.heading--three.ui-list--charlie__title.right Competition
            %span.ui-list--charlie__meta.left{"data-ui-component" => "schedule-time"} 7:45pm
          %li.ui-list--charlie__item
            %h3.heading--three.ui-list--charlie__title.right.text-left Food
            %span.ui-list--charlie__meta.left{"data-ui-component" => "schedule-time"} 8:00pm
          %li.ui-list--charlie__item
            %h3.heading--three.ui-list--charlie__title.right.text-left Networking
            %span.ui-list--charlie__meta.left{"data-ui-component" => "schedule-time"} 8:30pm
      / /schedule
      / about
      #about.ui-container--delta.photo-container--alpha{"data-ui-component" => "about-container"}
        .text-overlay
          .ui-container--charlie
            %h1.heading--four.spacing-bravo.heading-underline.text-light About Refresh Teesside
          .ui-container--alpha.spacing-charlie
            %p.copy--alpha.text-light= @about.description
          %a.ui-button--alpha{href: "#contact"} Submit Talk Idea
      / /about
      / sponsor
      #sponsors.ui-container--delta.alt-container{"data-ui-component" => "sponsor-container"}
        .ui-container--charlie
          %h1.heading--four.spacing-bravo.heading-underline Sponsors
        .ui-container--alpha
          %ul.ui-list--bravo{"data-ui-component" => "speaker-list"}
            %li.ui-list--bravo__item.grid-one-quarter.left
              .ui-list__item__image.spacing-bravo
                =image_tag("Digital-City-Business-logo-300x163.jpg")
            / %li.ui-list--bravo__item.grid-one-quarter.left
            /   .ui-list__item__image.spacing-bravo
            /     %img{alt: "sponsor", src: "http://placehold.it/150x150"}/
      / /sponsor
      / contact
      #contact.ui-container--delta--alt.ui-container--delta--alt{"data-ui-component" => "contact-container"}
        .ui-container--charlie
          %h1.heading--four.spacing-bravo.heading-underline Get in Touch
        .ui-container--alpha
          %p.copy--alpha.spacing-bravo Our aim is to create a buzzing atmosphere that encourages collaboration, learning and opportunity so if you think of anything that will improve the event, would like to give a talk(doesn't matter if its your first time), sponsor a prize or think you can offer help in any other way please let us know.
          %form.ui-form.spacing-charlie{"accept-charset" => "UTF-8", action: "/messages", "data-remote" => "true", "data-ui-component" => "contact-form", method: "post"}
            = token_tag nil
            %fieldset
              .ui-form__item.grid-one-whole.spacing-alpha
                %label.label--alpha.grid-one-whole{for: "name"} Name
                .ui-form__input.grid-one-whole
                  %input.grid-one-whole.js-name{name: "message[name]", placeholder: "Please enter your name", type: "text"}/
              .ui-form__item.grid-one-whole.spacing-alpha
                %label.label--alpha.grid-one-whole{for: "name"} Email
                .ui-form__input.grid-one-whole
                  %input.grid-one-whole.js-email{name: "message[email]", placeholder: "Please enter your email", type: "text"}/
              .ui-form__item.grid-one-whole.spacing-alpha
                %label.label--alpha.grid-one-whole{for: "name"} Subject
                .ui-form__input.grid-one-whole
                  %select#enquiry.select--alpha.grid-three-tenths--break.js-subject{name: "message[subject]"}
                    %option{value: "general-enquiry"} Enquiry type...
                    %option{value: "general-enquiry"} General Enquiry
                    %option{value: "talk-submit"} Submit Talk Idea
                    %option{value: "Jobs"} Jobs

              .ui-form__item.grid-one-whole.spacing-alpha
                %label.label--alpha.grid-one-whole{for: "name"} Message
                .ui-form__input.grid-one-whole
                  %textarea.grid-one-whole.js-body{name: "message[body]", placeholder: "Please enter your message", rows: "8"}
              .ui-form__item.grid-one-half--break
                %input.grid-one-half.ui-button--alpha{name: "submit", type: "submit", value: "Submit"}
            %p.copy--alpha.spacing-bravo.js-message-holder

        %iframe{frameborder: "0", height: "450", src: "https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2312.5733933886313!2d-1.235165999999969!3d54.576269999999866!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x487eed122374caf7%3A0x24872a99099d158!2sMink+Tattoo+Coffee+Bar!5e0!3m2!1sen!2suk!4v1396641446479", style: "border:0;display:block;", width: "100%"}

        .ui-container--delta.alt-container
          %ul.ui-list--delta
            %li.ui-list--delta__item
              %a{href: "http://www.twitter.com/refreshteesside", title: "Twitter"}
                %i.icon--twitter.icon--large
            %li.ui-list--delta__item
              %a{href: "http://www.facebook.com/refreshteesside", title: "Facebook"}
                %i.icon--facebook.icon--large
            %li.ui-list--delta__item
              %a{href: "http://groups.google.com/forum/#!forum/refresh-teesside", title: "Google Groups"}
                %i.icon--google.icon--large
